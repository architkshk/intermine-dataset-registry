<!DOCTYPE html>
<html>
<%- include('header.ejs') %>
<body>
	<nav class="navbar navbar-default pb-10" id="principal-nav">
		<div class="col-md-8">
			<ul class="nav navbar-nav" id="normal-title">
				<li>
					<a href="/" style="padding-bottom: 0px; padding-top: 0px;">
						<h2 id="title" class="ml-20 bold">InterMine Datasets</h2>
					</a>
				</li>
				<li>
					<div>
						<p id="title-desc" class="vertical-align-middle align-center mt-30">All InterMine datasets and its datasources.</p>
					</div>
				</li>
			</ul>
			<ul class="nav navbar-nav" id="responsive-title">
				<li>
					<a href="/" style="padding-bottom: 0px; padding-top: 0px;">
						<h2 id="title" class="ml-20 bold">InterMine Datasets</h2>
					</a>
				</li>
				<li>
					<div>
						<p id="title-desc" class="vertical-align-middle align-center mt-30">All InterMine datasets and its datasources.</p>
					</div>
				</li>
			</ul>
		</div>
		<div class="col-md-4">
			<div class="col-xs-6">
				<div class="card bg-success" style="width: 5vm; margin: 10px">
					<div class="card-body">
						<div class="col-xs-6">
							<div class="rotate">
								<img class="card-img-top" src="/images/interminelogo.png" width="30" height="30" alt="Mines">
							</div>
							<h6 class="text-uppercase">Mines</h6>
						</div>
						<div class="col-xs-6">
							<h1 class="display-4 align-center" id="mines-count">0</h1>
						</div>
					</div>
				</div>
			</div>
			<div class="col-xs-6">
				<div class="card bg-success" style="width: 5vm; margin: 10px">
					<div class="card-body">
						<div class="col-xs-5">
							<div class="rotate">
								<img class="card-img-top" src="/images/database_icon.png" width="30" height="30" alt="Datasets">
							</div>
							<h6 class="text-uppercase ">Datasets</h6>
						</div>
						<div class="col-xs-7">
							<h1 class="display-4 align-center" id="datasets-count">0</h1>
						</div>
					</div>
				</div>
			</div>
		</div>

	</nav>
	<nav class="navbar navbar-default pb-15 animated fadeInUp" id="tools-bar">

    <div class="row ml-10">

						<div class="col-md-10">
		            <div class="form-group animated fadeInLeft">
		                <input type="text" class="form-control" id="search-dataset" placeholder="Search for a dataset..." />
		            </div>
		        </div>
    </div>

  </nav>

    <section id="dataset-body">
			<div id="dimmer"></div>
			<div id="loader"></div>
			<div id="loader-text" class="align-center"> Synchronizing Datasets </div>
    	<div class="container pt-20">
    		<div class="row mt-xs-20 mt-md-20 mt-lg-20 pb-50">
    			<div class="col-lg-10 col-lg-offset-1">
						<div id="successAlertDiv">
						</div>

    				<!-- List View -->
            <div class='pt-20'> </div>
    				<div class="tab-content mt-lg-20">
    					<div role="tabpanel" class="tab-pane fade in active" id="datasets-list">
                <table style="width:100%" class="table table-hover table-responsive">
                  <thead>
                    <tr>
                      <th class="col-md-3">Name</th>
                      <th class="col-md-5">Description</th>
                      <th class="col-md-3">Url</th>
                      <th class="col-md-2 ">Mine</th>
                    </tr>
                  </thead>
                  <tbody id="list-table-body">

                  </tbody>
                </table>

                <div id="mine-modals" class="modal fade" tabindex="-1" role="dialog">
                  <div class="modal-dialog">
                  	<div class="modal-content">
                  		<div class="modal-header" id="list-modal-header">
                  		<button type="button" class="close closeb" data-dismiss="modal">×</button>
                  			<h3 id="modal-mine-title" style="display: inline;">Modal header</h3>
                  	  </div>
                    	<div class="modal-body pb-30" id="mine-modal-body">

                    	</div>
						<div class="modal-footer pb-15">
							<button class="btn btn-raised closeb mr-10 close-btn" data-dismiss="modal">
								<span class="text">Close</span>
							</button>
                    	</div>
                  	</div>
                  </div>
                </div>

                <div id="delete-modal" class="modal fade" tabindex="-1" role="dialog">
                  <div class="modal-dialog">
                  	<div class="modal-content">
                  		<div class="modal-header">
	                  		<button type="button" class="close closeb" data-dismiss="modal">×</button>
                  			<h3 id="modal-delete-mine-title">Modal header</h3>
                  	  </div>
                    	<div class="modal-body mb-20" id="mine-delete-modal-body">
                    	</div>
                    	<div class="modal-footer pb-15">
                        <button class="btn btn-raised btn-danger confirmdeleteb" data-dismiss="modal">Confirm</button>
                    		<button class="btn btn-raised closeb mr-10" data-dismiss="modal">Close</button>
                    	</div>
                  	</div>
                  </div>
                </div>


    					</div>

  						<div role="tabpanel" class="tab-pane fade" id="datasets-world">
                <div id="map" class="align-center"></div>
  						</div>

    				</div>
    			</div>
    		</div>
      </div>
    </section>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script type="text/javascript">
      var jQuery_1_9_1 = $.noConflict(true);
    </script>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script type="text/javascript">
      var jQuery_2_1_3 = $.noConflict(true);
    </script>
    <script language="javascript" src="/javascripts/headerAnimation.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
	<script language="javascript" src="/javascripts/jquery.animateNumber.min.js"></script>
    <script src="/js/material.js"></script>
		<script src="/js/ripples.min.js"></script>


    <script language="javascript" src="/js/bootstrap.min.js"></script>
    <script language="javascript" src="/javascripts/index.js"></script>
		<script>
		  $.material.init();
			$('[data-toggle="tooltip"]').tooltip();
		</script>
    <script type="text/javascript">
			if (localStorage.getItem("message")){
				$("#successAlertDiv").append( '<div class = "successalert" id = "indexActionAlert"> ' +
												'<span class="closebtn" onclick="this.parentElement.style.display='+"'none'" + ';">&times;</span> ' +
												localStorage.getItem("message") 		 +
												'</div>'
											);
				localStorage.clear();
			}
    </script>


  </body>
</html>
